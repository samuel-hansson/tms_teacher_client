{
  "name": "tms_teacher_client",
  "version": "1.0.0",
  "description": "teaching management system teacher client",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "package-mac": "electron-packager . --overwrite --platform=darwin --arch=x64 --icon=assets/icons/mac/icon.icns --prune=true --out=release-builds",
    "package-win": "electron-packager . tms_teacher_client --overwrite --asar=true --platform=win32 --arch=ia32 --icon=assets/icons/win/teaching.ico --prune=true --out=release-builds --version-string.CompanyName=CE --version-string.FileDescription=CE --version-string.ProductName=\"tms_teacher_client\"",
    "package-linux": "electron-packager . electron-tutorial-app --overwrite --asar=true --platform=linux --arch=x64 --icon=assets/icons/png/1024x1024.png --prune=true --out=release-builds"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/samuel-hansson/tms_teacher_client.git"
  },
  "author": "samuel hansson",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/samuel-hansson/tms_teacher_client/issues"
  },
  "homepage": "https://github.com/samuel-hansson/tms_teacher_client#readme",
  "dependencies": {
    "agora-electron-sdk": "^3.1.2-rc231-build.981"
  },
  "devDependencies": {
    "electron": "^10.1.4",
    "electron-packager": "^15.1.0"
  },
  "optionalDependencies": {
    "bufferutil": "^4.0.2",
    "utf-8-validate": "^5.0.3"
  }
}
